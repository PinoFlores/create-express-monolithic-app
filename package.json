      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/inquirer": "^8.1.3",
    "@types/shelljs": "^0.8.11",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
